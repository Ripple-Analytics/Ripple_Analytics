{application, gdrive_source_service,
 [{description, "Google Drive Update Source Service"},
  {vsn, "1.0.0"},
  {registered, [gdrive_worker]},
  {mod, {gdrive_source_app, []}},
  {applications, [kernel, stdlib, inets, crypto, cowboy, jsx]},
